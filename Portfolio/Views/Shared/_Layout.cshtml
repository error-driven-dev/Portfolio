<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/CSS/Styles.css" rel="stylesheet" />
    
</head>
<body>
<nav class="navbar navbar-expand-md">
    <a class="navbar-brand" src="~/images/nameplate.png" href="/"><img class="nameplate" src="~/images/nameplateAngle.png"></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav ">
        <li class="nav-item">
            <a class="nav-link" asp-action="Index"><img class="pagelinks" src="~/images/AboutMe.png"></a>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-action="Projects"><img class="pagelinks" src="~/images/Samples.png" ></a>
        </li>
        <li class="nav-item">
            <a class="nav-link" asp-action="Contact"><img class="pagelinks" src="~/images/Contact.png"></a>
        </li>
    </ul>
    </div>

</nav>
<div>
    @RenderBody()
</div>
</body>
</html>
